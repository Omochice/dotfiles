{
  "marp's yaml option": {
    "prefix": "marp.bgse",
    "body": [
      "---",
      "marp: true",
      "paginate: true",
      "theme: bgse",
      "size: 4:3",
      "---",
      "$1"
    ]
  },
  "reveal.js's yaml option": {
    "prefix": "reveal",
    "body": [
      "---",
      "theme: simple",
      "transition: 'slide'",
      "slideNumber: true",
      "enableTitleFooter: false",
      "---",
      "$1"
    ]
  },
  "marp's lead class setting": {
    "prefix": "class.lead",
    "body": [
      "<!-- _class: lead -->",
      "$1"
    ]
  },
  "gh-page header": {
    "prefix": "gh-pages",
    "body": [
      "---",
      "layout: post",
      "title: \"$1\"",
      "category: \"\"",
      "excerpt: \"\"",
      "---",
      "",
      "# {{ page.title }}"
    ]
  },
  "link template": {
    "prefix": "link",
    "body": [
      "[$1]($2)"
    ]
  }
}
