{
  "Println": {
    "prefix": "fmt",
    "body": [
      "fmt.Println($1)"
    ]
  },
  "main": {
    "prefix": "main",
    "body": [
      "package main;",
      "",
      "import \"fmt\";",
      "",
      "func main() {",
      "  $1",
      "};"
    ]
  },
  "error handling": {
    "prefix": [
      "if",
      "iferr"
    ],
    "body": [
      "if err != nil {",
      "  $1",
      "}"
    ]
  },
  "for-range": {
    "prefix": [
      "forr"
    ],
    "body": [
      "for i, ${1:v} := range $2 {",
      "  $3",
      "}"
    ],
    "description": "for-range"
  },
  "for-loop": {
    "prefix": "for",
    "body": [
      "for ${1:i} := 0; $1${2:expr}; $1++ {",
      "  $3",
      "}"
    ],
    "description": "for-loop"
  },
  "define func": {
    "prefix": [
      "def",
      "func"
    ],
    "body": [
      "func ${1:name}(${2:args}) ${3:returns} {",
      "\t$4",
      "}"
    ],
    "description": "define function"
  }
}
