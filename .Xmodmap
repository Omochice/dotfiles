! caps -> ctrl
clear lock
clear control
keycode 66 = Control_L
add control = Control_L Control_R

! 半角全角 -> ECS
keycode 49 = Escape

! 変換 -> 半角全角
keycode 100 = Zenkaku_Hankaku

! 無変換 -> ESC
keycode 102 = Escape
!keysym Muhenkan = Escape

! カタカナひらがな -> super_L
keycode 101= Super_L
!keysym Hiragana_Katakana = Super_L
